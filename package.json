{
  "name": "hailen-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/Main/index.ts",
  "types": "./typings/index.d.ts",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"vite\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development yarn electron\"",
    "build": "vite build && tsc",
    "electron": "electron ./dist/Main/index.js"
  },
  "build": {
    "appId": "com.github.hailen-client",
    "productName": "Hailen-Client",
    "copyright": "Copyright Â© 2024 Hailen",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "src/Main/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "output"
    }
  },
  "dependencies": {
    "koa": "^2.15.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/koa": "^2.15.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0"
  },
  "license": "MIT"
}
