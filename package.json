{
  "name": "hailen-client",
  "private": true,
  "version": "1.0.0",
  "description": "Hailen Client",
  "main": "index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "start": "node electron-cli-service start",
    "build": "node electron-cli-service build",
    "dev:vue": "vite --config vite.config.ts --mode vue",
    "dev:react": "vite --config vite.config.ts --mode react",
    "build:vue": "vite build --config vite.config.ts --mode vue",
    "build:react": "vite build --config vite.config.ts --mode react",
    "lint": "eslint ."
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "antd": "^5.21.6",
    "axios": "^1.7.8",
    "bcrypt": "5.0.0",
    "colors": "^1.4.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.8",
    "element-plus": "^2.8.7",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^13.0.1",
    "koa-static": "^5.0.0",
    "lunar-calendar": "^0.1.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "vue": "^3.4.37",
    "vue-router": "^4.4.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^22.5.5",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue/babel-preset-jsx": "^1.4.0",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.7",
    "sass": "^1.80.7",
    "sass-loader": "^16.0.3",
    "shelljs": "^0.8.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^2.0.29",
    "wait-on": "^8.0.1"
  },
  "author": "Hailen",
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": ""
      }
    ],
    "asar": true,
    "asarUnpack": [
      "public",
      "node_modules/@spk"
    ],
    "appId": "com.hailen-client",
    "productName": "海阔天空",
    "files": [
      "dist",
      "public",
      "index.js",
      "config.js",
      "!**/*.{lib,obj,pdb,cc,h,tlog,map,exp,gypi,vcxproj,filters,cpp,c,ts}"
    ],
    "nodeGypRebuild": false,
    "extraFiles": [
      "source"
    ],
    "win": {
      "requestedExecutionLevel": "asInvoker",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip"
        }
      ],
      "icon": "public/favicon/ico/favicon@5x.ico"
    },
    "mac": {
      "icon": "public/favicon/icons/favicon@5x.icns"
    },
    "linux": {
      "icon": "public/favicon/icons/favicon@5x.icns",
      "category": "AudioVideo",
      "target": "deb",
      "executableName": "海阔天空"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "installerLanguages": "zh-CN"
    },
    "directories": {
      "output": "output"
    }
  },
  "license": "ISC"
}
